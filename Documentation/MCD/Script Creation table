CREATE DATABASE nom_base;

CREATE TABLE employe
(
    Idemp INT PRIMARY KEY,
    IdLig INT FOREIGN KEY REFERENCES ligue(IdLig)
    nomEmp VARCHAR(50),
    preEmp VARCHAR(50),
    mail VARCHAR(100),
    Password VARCHAR(100),
    droitRoot BOOLEAN,
    dateArrive DATE,
    dateDepart DATE
)

CREATE TABLE ligue
(
    IdLig INT PRIMARY KEY,
    nomLig VARCHAR(50)
   )

